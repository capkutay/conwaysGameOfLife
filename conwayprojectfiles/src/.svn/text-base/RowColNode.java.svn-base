
public class RowColNode {
	private int rowColNumber;
	private MyMatrixElem matrixElem;
	private RowColNode next; 
	
	public RowColNode(int rowColNumber){
		this.rowColNumber = rowColNumber;
	}
	
	public int getIndex(){
		return rowColNumber;
	}
	
	public RowColNode getNext(){
		return next;
	}
	
	public MyMatrixElem getElem(){
		return matrixElem;
	}

	public void setNext(RowColNode nextRowNode) {
		this.next = nextRowNode;
	}
	
	public void setMatrixElem(MyMatrixElem matrixElem){
		this.matrixElem = matrixElem;
	}
	

	

}
